// promises : 3 states : pending, fulfilled, rejected

function getdata() {
    return new Promise((resolve, reject) => {
        setTimeout(() => {
            resolve('data fetched');
        },3000);
    });    
}
getdata() 
    .then(result =>{
        console.log(result);
    })
    .catch(error => {
        console.error(error);
    })
,

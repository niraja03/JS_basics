// fn + lexical scope = closure
// lex//lexical scope is if value is not present within brackets then it come out od brac and checks

function outer() {
    let count = 0;
    function inner(){
        console.log(count);
        count++;
    }
    return inner;
}

let counterincrement = outer() ;
counterincrement();
counterincrement();
counterincrement();
counterincrement();
counterincrement();
